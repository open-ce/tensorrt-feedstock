From 538a5b5399507f9219bcd73ce7e5e1342184f034 Mon Sep 17 00:00:00 2001
From: "Richard J. Knight" <rjknight@us.ibm.com>
Date: Wed, 2 Oct 2019 10:47:27 -0400
Subject: [PATCH] Add makefiles for new samples

---
 samples/opensource/sampleUffFasterRCNN/Makefile | 5 +++++
 samples/opensource/sampleUffMaskRCNN/Makefile   | 5 +++++
 2 files changed, 10 insertions(+)
 create mode 100755 samples/opensource/sampleUffFasterRCNN/Makefile
 create mode 100644 samples/opensource/sampleUffMaskRCNN/Makefile

diff --git a/samples/opensource/sampleUffFasterRCNN/Makefile b/samples/opensource/sampleUffFasterRCNN/Makefile
new file mode 100755
index 0000000..225d9b8
--- /dev/null
+++ b/samples/opensource/sampleUffFasterRCNN/Makefile
@@ -0,0 +1,5 @@
+OUTNAME_RELEASE = sample_uff_fasterRCNN
+OUTNAME_DEBUG   = sample_uff_fasterRCNN_debug
+EXTRA_DIRECTORIES = ../common
+MAKEFILE ?= ../Makefile.config
+include $(MAKEFILE)
diff --git a/samples/opensource/sampleUffMaskRCNN/Makefile b/samples/opensource/sampleUffMaskRCNN/Makefile
new file mode 100644
index 0000000..28c383a
--- /dev/null
+++ b/samples/opensource/sampleUffMaskRCNN/Makefile
@@ -0,0 +1,5 @@
+OUTNAME_RELEASE = sample_uff_maskRCNN
+OUTNAME_DEBUG   = sample_uff_maskRCNN_debug
+EXTRA_DIRECTORIES = ../common
+MAKEFILE ?= ../Makefile.config
+include $(MAKEFILE)
